<div id="iamacrew" class="page_IamA">
  <section class="banner banner_iamacrew" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path @banner_url %>')" >
    <div class="banner_content">
      <h1><%= t('iamacrew.title') %></h1>
    </div>
  </section>
  <section class="question_left section section_red_shape_photo_right section_path section_left_path" >
    <div class="container">
      <h2><%= t('iamacrew.q1') %></h2>
      <p><%= t('iamacrew.a1_html') %></p>
      <h2><%= t('iamacrew.q2') %></h2>
      <p><%= t('iamacrew.a2_html') %></p>
    </div>
  </section>
  <section class="question_right section section_yellow1_left" >
    <div class="container">
      <h2><%= t('iamacrew.q3') %></h2>
      <p><%= t('iamacrew.a3_html') %></p>
      <h2><%= t('iamacrew.q4') %></h2>
      <p><%= t('iamacrew.a4_html') %></p>
    </div>
  </section>

  <section class="call2action" >
    <!-- Begin MailChimp Signup Form -->
    <div id="mc_embed_signup">
      <form action="//packyourskills.us14.list-manage.com/subscribe/post?u=8c95cf695d48cd7d49d9b5812&amp;id=356d8c6259" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
        <div id="mc_embed_signup_scroll">
          <div class="mc-field-group">
            <h2><%= t('iamacrew.participate') %></h2>
            <p><%= t('iamacrew.email') %></p>
            <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL">
          </div>
          <div class="mc-field-group">
            <p><%= t('iamacrew.name') %></p>
            <input type="text" value="" name="FNAME" class="required" id="mce-FNAME">
          </div>
          <div class="mc-field-group">
            <p><%= t('iamacrew.city') %></p>
            <input type="text" value="" name="LNAME" class="required" id="mce-LNAME">
          </div>
          <div id="mce-responses" class="clear">
            <div class="response" id="mce-error-response" style="display:none"></div>
            <div class="response" id="mce-success-response" style="display:none"></div>
          </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_8c95cf695d48cd7d49d9b5812_356d8c6259" tabindex="-1" value=""></div>
          <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="btn btn-info"></div>
        </div>
      </form>
    </div>
    <!--End mc_embed_signup-->
  </section>
</div>
