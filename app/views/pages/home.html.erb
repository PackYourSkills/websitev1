<!-- Banner Part -->
<section id="banner" class="section">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-6">
        <h1 class="slogan_part1"><%= t('home.banner.slogan1') %></h1>
        <h1 class="slogan_part1"><%= t('home.banner.slogan2') %> <%= image_tag "/home.can.#{I18n.locale}.png", id: 'img_can' %></h1>
        <h1 class="slogan_part2"><%= t('home.banner.slogan3') %></h1>
        <h1 class="slogan_part2"><%= t('home.banner.slogan4') %></h1>
        <h2><%= t('home.banner.baseline1') %></h2>
        <h2><%= t('home.banner.baseline2') %></h2>
      </div>
    </div>
    <div class="row row_call2action">
      <div class="col-xs-12 col-sm-6">
        <a class="red_call2action" href="#" role="button"><%= t('home.banner.call2action') %></a>
      </div>
    </div>
  </div>
</section>
<!-- Enf of Banner Part -->

<!-- Mission Part -->
<section id="mission" class="section section_path section_right_path">
  <div class="container">
    <div class="row">
      <div class="col-xs-9 col-sm-offset-1 col-sm-8 col-md-offset-2 col-md-6">
        <h1><%= t('home.mission.title') %></h1>
        <p><%= t('home.mission.text1') %></p>
        <p><%= t('home.mission.text2') %></p>
      </div>
      <div class="col-xs-3 col-sm-2 col-md-2">
        <%= image_tag "/home.question.png" %>
      </div>
    </div>
  </div>
</section>
<!-- Enf of Mission Part -->

